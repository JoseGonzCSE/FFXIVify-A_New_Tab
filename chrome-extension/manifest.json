{
  "manifest_version": 3,
  "name": "FFXIVify: A New Tab",
  "version": "1.2.0",
  "short_name": "FFXIVify",
  "description": "FFXIV your new tab! ",
  "permissions": ["storage","tabs","search"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
